    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "from keras.models import Sequential\n",
        "from keras.layers import Input, Conv2D, AveragePooling2D, Flatten, Dense\n",
        "from keras.optimizers import Adam\n",
        "from keras.callbacks import EarlyStopping\n",
        "\n",
        "lenet5 = Sequential([\n",
        "    Input(shape=(28, 28, 1)),\n",
        "\n",
        "    Conv2D(6, (5, 5), activation=\"tanh\", padding=\"same\"),\n",
        "    AveragePooling2D(pool_size=(2, 2)),\n",
        "\n",
        "    Conv2D(16, (5, 5), activation=\"tanh\"),\n",
        "    AveragePooling2D(pool_size=(2, 2)),\n",
        "\n",
        "    Flatten(),\n",
        "    Dense(120, activation=\"tanh\"),\n",
        "    Dense(84, activation=\"tanh\"),\n",
        "    Dense(10, activation=\"softmax\")\n",
        "])\n",
        "\n",
        "lenet5.compile(\n",
        "    optimizer=Adam(learning_rate=0.001),\n",
        "    loss=\"categorical_crossentropy\",\n",
        "    metrics=[\"accuracy\"]\n",
        ")\n",
        "\n",
        "lenet5.summary()\n",
        "\n",
        "early_stop = EarlyStopping(\n",
        "    monitor=\"val_accuracy\",\n",
        "    patience=3,\n",
        "    restore_best_weights=True\n",
        ")\n",
        "\n",
        "history_lenet5 = lenet5.fit(\n",
        "    x_train_cnn, y_train_cnn,\n",
        "    epochs=20,\n",
        "    batch_size=128,\n",
        "    validation_data=(x_test_cnn, y_test_cnn),\n",
        "    callbacks=[early_stop],\n",
        "    verbose=1\n",
        ")\n",
        "\n",
        "lenet5_loss, lenet5_acc = lenet5.evaluate(x_test_cnn, y_test_cnn, verbose=0)\n",
        "print(f\"\\nLeNet-5 Style Test Accuracy: {lenet5_acc:.4f} | Test Loss: {lenet5_loss:.4f}\")"
      ],
      "metadata": {
        "id": "NcpHe5r5P9hy",
        "outputId": "f89a146f-765a-4e42-f3eb-45427f71e569",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 875
        }
      },
